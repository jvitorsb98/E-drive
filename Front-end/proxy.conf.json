{
  "/api/maps": {
    "target": "https://maps.googleapis.com",
    "secure": true,
    "changeOrigin": true,
    "pathRewrite": {
      "^/api/maps": "/maps" 
    },
    "logLevel": "debug"
  },
  "/api/backend": {
    "target": "http://backend:8080",  
    "secure": false,
    "changeOrigin": true,
    "pathRewrite": {
      "^/api/backend": ""
    },
    "logLevel": "debug"
  }
}
